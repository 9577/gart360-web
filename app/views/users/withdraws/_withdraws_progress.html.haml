.progress
  .progress-bar.progress-bar-success{'aria-valuemax' => '100', 'aria-valuemin' => '0', 'aria-valuenow' => "40", role: 'progressbar', style: "width: #{state_for_progress(Withdraw::STATE_PROCESS, state)}%"}
    %span.sr-only 40% Complete (success)
.time-item.clearfix
  %dl.first-step{ class: Withdraw::STATE_PROCESS.index(state) >= 0 ? 'active' : '' }
    %dd 提交申请
    %dd.item
      %span.first-item
  %dl.second-step{ class: Withdraw::STATE_PROCESS.index(state) >= 1 ? 'active' : '' }
    %dd 客服受理
    %dd.item
      %span.second-item
  %dl.third-step{ class: Withdraw::STATE_PROCESS.index(state) >= 2 ? 'active' : '' }
    %dd 转账成功
    %dd.item
      %span.third-item
  %dl.fourth-step{ class: Withdraw::STATE_PROCESS.index(state) >= 3 ? 'active' : '' }
    %dd 客户确认
    %dd.item
      %span.fourth-item